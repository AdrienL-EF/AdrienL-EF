table OverallQuestionGroups_HasStaffTour

	column ProdTour_id
		dataType: int64
		isHidden
		sourceProviderType: Integer
		sourceColumn: ProdTour_id

	column HasStaffTour
		dataType: int64
		sourceProviderType: Boolean
		summarizeBy: none
		sourceColumn: HasStaffTour

	partition ProdTour_HasStaff = query
		dataView: full
		source
			query =
				SELECT ProdTour_id, CAST(HasStaffTour AS TINYINT) AS HasStaffTour
				FROM ProcessedReports_LandOps.dbo.factProdTours
				
				UNION
				
				SELECT ProdTour_id, CAST(HasStaffTour AS TINYINT) AS HasStaffTour
				FROM ProcessedReports_LandOps.dbo.factProdTours_History
			dataSource: 'SqlServer BIStaging'

		annotation QueryEditorSerialization =
